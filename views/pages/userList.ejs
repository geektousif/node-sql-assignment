    <title>Assignment</title>
</head>

<body>
    <div class="container mx-auto">

        <h1 class="my-5">User List</h1>

        <ul>
            <% users.forEach((user) => { %>
            <li>
                <h2>
                    <%= user.name %>
                </h2>
                ---
                <p> <%= user.email %> </p>
            </li>
            <hr />
            <% }) %>
        </ul>

    </div>

    <script>

    </script>
</body>

</html>